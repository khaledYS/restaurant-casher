{
  "name": "react",
  "version": "0.0.0",
  "scripts": {
    "vite:watch": "vite",
    "vite:watch:host": "vite --host",
    "vite:watch:domain:default": "vite --port 80",
    "vite:build": "vite build",
    "vite:preview": "vite preview",
    "tailwind:watch": "npx tailwindcss -i ./src/styles/App.css -o ./src/tailwind/output.css --watch",
    "tailwind:build": "npx tailwindcss -o ./src/tailwind/output.css --minify",
    "build:all": "npm run tailwind:build && npm run vite:build",
    "build:all:preview": "npm run build:all && npm run vite:preview",
    "dev": "start npm run vite:watch && npm run tailwind:watch "
  },
  "dependencies": {
    "firebase": "^9.6.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.1.4",
    "tailwindcss": "^3.0.8",
    "vite": "^2.7.2"
  }
}
